		</xsd:complexType>
	</xsd:element>
	
    <xsd:element name="deployApplicationResponse">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="response" type="xsd:string"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>	
	
	<xsd:element name="redeployApplicationRequest">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="module:UserBaseRequest">
